{% extends 'adminbase.html.twig' %}
{% block title %}Kategori Detayı{% endblock %}
{% block content %}
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper site-min-height">
            <h3><i class="fa fa-angle-right"></i> Kategori Detayı </h3>
            <div class="row mt">
                <div class="col-lg-12">
                    <div class="col-md-12">
                        <div class="content-panel">

                            <table class="table">
                                <tbody>
                                <tr>
                                    <th>Id</th>
                                    <td>{{ setting.id }}</td>
                                </tr>
                                <tr>
                                    <th>Title</th>
                                    <td>{{ setting.title }}</td>
                                </tr>
                                <tr>
                                    <th>Description</th>
                                    <td>{{ setting.description }}</td>
                                </tr>
                                <tr>
                                    <th>Keywords</th>
                                    <td>{{ setting.keywords }}</td>
                                </tr>
                                <tr>
                                    <th>Company</th>
                                    <td>{{ setting.company }}</td>
                                </tr>
                                <tr>
                                    <th>Address</th>
                                    <td>{{ setting.address }}</td>
                                </tr>
                                <tr>
                                    <th>Fax</th>
                                    <td>{{ setting.fax }}</td>
                                </tr>
                                <tr>
                                    <th>Phone</th>
                                    <td>{{ setting.phone }}</td>
                                </tr>
                                <tr>
                                    <th>Email</th>
                                    <td>{{ setting.email }}</td>
                                </tr>
                                <tr>
                                    <th>Smtserver</th>
                                    <td>{{ setting.smtserver }}</td>
                                </tr>
                                <tr>
                                    <th>Smtpemail</th>
                                    <td>{{ setting.smtpemail }}</td>
                                </tr>
                                <tr>
                                    <th>Smptppass</th>
                                    <td>{{ setting.smptppass }}</td>
                                </tr>
                                <tr>
                                    <th>Smtpport</th>
                                    <td>{{ setting.smtpport }}</td>
                                </tr>
                                <tr>
                                    <th>Aboutus</th>
                                    <td>{{ setting.aboutus }}</td>
                                </tr>
                                <tr>
                                    <th>Contact</th>
                                    <td>{{ setting.contact }}</td>
                                </tr>
                                <tr>
                                    <th>Referances</th>
                                    <td>{{ setting.referances }}</td>
                                </tr>
                                <tr>
                                    <th>Status</th>
                                    <td>{{ setting.status }}</td>
                                </tr>
                                </tbody>
                            </table>

                            <a href="{{ path('admin_setting_index') }}">Geri</a>

                            <a href="{{ path('admin_setting_edit', {'id': setting.id}) }}">edit</a>

                            {{ include('admin/setting/_delete_form.html.twig') }}
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- /wrapper -->
    </section>
    <!-- /MAIN CONTENT -->
    <!--main content end-->
{% endblock %}